%%{
	machine tsip_machine_parser_header_User_Agent;

	# Includes
	include tsip_machine_utils "./tsip_machine_utils.jrl";
	
	action tag{
		i_tag_start = p;
	}

	action parse_user_agent{
	    hdr_user_agent.s_value = tsk_ragel_parser_get_string(s_str, p, i_tag_start);
	}

	action eob{
	}
		
	User_Agent = "User-Agent"i HCOLON (js_any*)>tag %parse_user_agent;
	
	# Entry point
	main := User_Agent :>CRLF @eob;

}%%

%%write data;

function tsip_header_User_Agent(s_ua){
	this.__proto__.__proto__ = new tsip_header(tsip_header_type_e.User_Agent);
    this.s_value = s_ua;
	this.toString = function(){
		return this.s_value;
	};
}

tsip_header_User_Agent.prototype.Parse = function(s_str){
    var cs = 0;
	var p = 0;
	var pe = s_str.length;
	var eof = pe;
	var data = tsk_buff_str2ib(s_str);
	var i_tag_start;	
	var hdr_user_agent = new tsip_header_User_Agent(null);
	
	%%write init;
	%%write exec;
	
	if( cs < %%{ write first_final; }%% ){
		console.error("Failed to parse 'User-Agent' header: %s", s_str);
		return null;
	}
	
	return hdr_user_agent;
}